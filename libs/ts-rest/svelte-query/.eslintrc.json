{
  "extends": ["../../../.eslintrc.json"],
  "plugins": ["svelte3"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.js", "*.svelte"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ]  
}
